VERSION BUILD=8920312 RECORDER=FX

' MUST BE SET!
' EXAMPLE: SET PATH_TO_DATASOURCES_BRAZIL_STATES_CSV ~/brazil_states.csv
SET PATH_TO_DATASOURCES_BRAZIL_STATES_CSV

' MUST BE SET!
