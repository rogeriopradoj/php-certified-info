ADD !EXTRACT {{URL}}
SAVEAS TYPE=EXTRACT FOLDER={{PATH_TO_RAWRESULTS_FOLDER}} FILE=raw_brazil_states_v{{!NOW:yyyy-mm-dd}}.csv
TAB CLOSE
